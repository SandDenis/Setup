<h1>Create a new category</h1>


<%= render 'shared/errors', obj: @category %>
<%= form_with(model: @category, local: true) do |f| %>

<p>
    <%= f.label :name, "Category name" %>
    <br/>
    <%= f.text_field :name %>
</p>


<p>
    <%= f.submit  %>
</p>

<%end%>

<%= link_to 'Return to articles listing',articles_path%>